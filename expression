(define (atom)
  (uniform-draw '("x" "1" "2" "3" "4")))

(define (expression)
  (uniform-draw 
   (list (atom)
      (string-append (atom) " + " (atom))
      (string-append (atom) " - " (atom))
      (string-append (atom) " * " (atom))
      (string-append (atom) " / " (atom))
      )))

(expression)
